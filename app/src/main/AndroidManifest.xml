        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CandidateLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CandidateRegistrationActivity" />
        <activity android:name=".CandidateUsersListUpdateActivity" />
        <activity android:name=".CandidateEducationalDetailsActivity" />
        <activity android:name=".B1AndB3Activity"
            android:label="B1AndB3Activity"/>
        <activity android:name=".B2AndB3Activity"
            android:label="B2AndB3Activity"/>
        <activity android:name=".ForgotActivity"/>
        <activity android:name=".B3Activity"/>
        <activity android:name=".UserDetailsActivity"/>
    </application>

</manifest>